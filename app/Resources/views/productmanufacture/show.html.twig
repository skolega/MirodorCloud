{% extends 'base.html.twig' %}

{% block body %}
    <h1>ProductManufacture</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ productManufacture.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ productManufacture.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ productManufacture.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ productManufacture.telephone }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ productManufacture.address }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('productmanufacture_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('productmanufacture_edit', { 'id': productManufacture.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
